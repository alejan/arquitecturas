extends layout

block content
    script(src='mainLatidos.js')
    h1 Latidos
    #messages
    #messagesrespuesta
    br
    form(onsubmit='return addLatidos(this)')
        input#IdMascota(type='text', placeholder='IdMascota')
        input#IdCollar(type='text', placeholder='IdCollar')
        input#IdUsuario(type='text', placeholder='IdUsuario')
        input#Latido(type='text', placeholder='Latido')
        input(type='submit', value='Enviar!')
    form(onsubmit='return desconectar(this)')
        input(type='submit', value='Desconectar!')
    form(onsubmit='return conectar(this)')
        input(type='submit', value='Conectar!')